{
    "ConnectionStrings": {
        "DefaultConnection": "Server=;Database=;User ID=;Password=;Connection Timeout=30;MultipleActiveResultSets=true"
    },

    "Logging": {
        "IncludeScopes": false,
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        },
        "Console": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },

    "Serilog": {
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/log-.txt",
                    "rollingInterval": "Day"
                }
            }
        ]
    },

    "AzureMediaStorageConfig": {
        "AccountName": "",
        "AccountKey": "",
        "Container": ""
    },

    "AzureOrderFileStorageConfig": {
        "AccountName": "",
        "AccountKey": "",
        "Container": ""
    },

    "ApplicationInsights": {
        "InstrumentationKey": ""
    }
}
